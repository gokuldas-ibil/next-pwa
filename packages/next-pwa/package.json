{
  "name": "@gokuldas/next-pwa",
  "version": "2.10.8",
  "type": "module",
  "private": false,
  "description": "Forked and customized version of next-pwa for gokuldas",
  "keywords": [
    "nextjs",
    "pwa",
    "workbox",
    "web",
    "service-worker"
  ],
  "author": "gokuldas <gokuldas.ibilglobal.com>",
  "license": "MIT",
  "repository": "https://github.com/gokuldas-ibil/next-pwa.git",
  "bugs": "https://github.com/gokuldas-ibil/next-pwa/issues",
  "homepage": "https://github.com/gokuldas-ibil/next-pwa#readme",
  "files": [
    "dist",
    "!dist/dts"
  ],
  "source": "./src/index.ts",
  "imports": {
    "#utils/*.js": "./src/utils/*.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./workbox": {
      "require": {
        "types": "./dist/sw-entry.d.cts"
      },
      "default": {
        "types": "./dist/sw-entry.d.ts"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "workbox": [
        "./dist/sw-entry.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "rimraf dist && rollup --config --watch",
    "build": "rimraf dist && NODE_ENV=production rollup --config --compact",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@gokuldas/next-pwa": "github:gokuldas-ibil/next-pwa#2.10.8",
    "fast-glob": "3.3.2",
    "semver": "7.6.3",
    "workbox-build": "7.1.1",
    "workbox-core": "7.1.0",
    "workbox-webpack-plugin": "7.1.0",
    "workbox-window": "7.1.0"
  },
  "devDependencies": {
    "@serwist/configs": "workspace:*",
    "@types/node": "22.5.5",
    "@types/semver": "7.5.8",
    "chalk": "5.3.0",
    "next": "14.2.12",
    "rollup": "3.28.1",
    "react": "19.0.0-rc-5dcb0097-20240918",
    "react-dom": "19.0.0-rc-5dcb0097-20240918",
    "type-fest": "4.26.1",
    "typescript": "5.7.0-dev.20240918",
    "webpack": "5.94.0"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "webpack": ">=5.9.0"
  }
}
